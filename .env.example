
# Example .env file for AWS ECR login cron container
# NOTE: To use these variables in cron jobs, you must export them or source this file in your cron command.
# For example, in your cron file:
# 0 */8 * * * root . /app/.env && aws ecr get-login-password --region "$AWS_REGION" | docker login --username AWS --password-stdin "$ECR_ADDRESS"

AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1
ECR_ADDRESS=your-account-id.dkr.ecr.us-east-1.amazonaws.com
